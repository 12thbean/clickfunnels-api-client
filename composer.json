{
    "name": "zendrop/clickfunnels-api-client",
    "description": "The package allows you to easily make requests to the ClickFunnels.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Zendrop\\ClickFunnelsApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zendrop\\ClickFunnelsApiClient\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "MrNubOK",
            "email": "vadim@zendrop.com"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "squizlabs/php_codesniffer": "^3.8",
        "phpunit/phpunit": "9.6.x-dev",
        "signifly/php-config": "^1.0",
        "phpstan/phpstan": "^1.10"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "zendrop/data": "^2.0.1",
        "illuminate/http": "^9.0"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
